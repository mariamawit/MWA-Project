<!DOCTYPE html>
<html>
  <head>
    <title>Users</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
      <table class="">
          <thead>
            <tr>
              <th>ID</th>
              <th>Name</th>
              <th>Username</th>
              <th>Email</th>
              <th>City</th>
              <th>Zip</th>
              <th>Latitude</th>
              <th>Longtiude</th>
              <th>Website</th>
            </tr>
          </thead>
          <tbody>
             <% for(let i in userdata){  %>
            <tr>
              <td><%= userdata[i].id %></td>
              <td><%= userdata[i].name %></td>
              <td><%= userdata[i].username %></td>
              <td><%= userdata[i].email %></td>
              <td><%= userdata[i].address.city %></td>
              <td><%= userdata[i].address.zipcode %></td>
              <td><%= userdata[i].address.geo.lat %></td>
              <td><%= userdata[i].address.geo.lng %></td>
              <td><%= userdata[i].website %></td>
            </tr>
            <%  } %>
          </tbody>
        </table>
  </body>
</html>
